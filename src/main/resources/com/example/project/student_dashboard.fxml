<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootVBox" alignment="CENTER" fillWidth="false" prefHeight="260" prefWidth="400" spacing="12.0" style="-fx-background-color: #f5f5f5;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.StudentDashboardController">

    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <TextField fx:id="roomIdField" prefHeight="30.0" prefWidth="120.0" promptText="Enter room id" />

    <TextField fx:id="nameField" prefHeight="30.0" prefWidth="120.0" promptText="Enter your name" />

    <Button onAction="#onStartExamClicked" prefHeight="26.0" prefWidth="88.0" text="Start Exam" />

</VBox>
